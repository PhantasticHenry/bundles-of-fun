<h3 class="login-heading mb-4"><%= @product_material.material.try(:name) %></h3>
<%= render partial: 'shared/errors', locals: {model: @product_material} %>
<%= render partial: 'shared/flash_errors', locals: {model: @product_material} %>
<table>
    <tr>
        <th>Quantity</th>
    </tr>
        <td><%= @product_material.quantity %></td>
    </tr>
</table>
<hr>
<%= link_to "Edit", edit_product_material_path(@product_material), class: "btn btn-primary" %>&nbsp;&nbsp;&nbsp; OR &nbsp;&nbsp;&nbsp;
<%= link_to "Delete", product_material_path(@product_material), method: :delete, class: "btn btn-primary",data: {confirm: "Are you sure?"} %><hr>
<%= link_to "Back", :back, class: "btn btn-primary" %>
